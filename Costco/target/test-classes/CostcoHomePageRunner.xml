<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Costco Automation " parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="OS X"/>
    <!--<parameter name="os_version" value="Mojave"/>-->
    <parameter name="browserName" value="chrome"/>

    <!--<parameter name="browserName" value="firefox"/>-->
    <!--<parameter name="browserVersion" value="72.0.3626.119"/>-->
    <parameter name="url" value="https://www.costco.com"/>

    <test name = "Costco Home">
        <classes>
            <class name="TestHomePage"/>
        </classes>
    </test>

</suite>